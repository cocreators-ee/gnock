{
  "name": "gnock",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm install && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint . --fix",
    "lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\" --fix",
    "eslint": "eslint . --fix",
    "pre-commit-lint:scss": "pnpm install && pnpm lint:scss",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.66.1",
    "shelljs": "^0.8.5",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.1.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "undici": "^5.24.0",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-static": "^2.0.3",
    "carbon-components-svelte": "^0.80.0",
    "carbon-icons-svelte": "^12.3.0"
  }
}
